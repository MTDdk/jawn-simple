<configuration>

    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} [%-8.8thread] %-5level %-20logger{30} - %msg%n</pattern>
        </encoder>
    </appender>

    
    <root level="DEBUG">
        <appender-ref ref="STDOUT" />
    </root>

</configuration>
